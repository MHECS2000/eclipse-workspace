package data;

public class DataTest {
	public static void main(String[] args) {
		
		DiaMesAno data1 = new DiaMesAno();
		data1.dia = 1;
		data1.mes = 3;
		data1.ano = 2000;
		
		var data2 = new DiaMesAno();
		data2.dia = 8;
		data2.mes = 5;
		data2.ano = 1993;
		
		String DT1 = data1.dataFormatada();
		String DT2 = data2.dataFormatada();
		
		
		System.out.println("primeira data "+ DT1);
		System.out.println("ssegunda data "+ DT2);
		
		
		
	}

}
